digraph Trie {
  rankdir=LR;
  splines=true;
  bgcolor="white";
  nodesep=0.35;
  ranksep=0.60;

  node [
    shape=circle,
    style=filled,
    fillcolor="#E3F2FD",
    color="#1565C0",
